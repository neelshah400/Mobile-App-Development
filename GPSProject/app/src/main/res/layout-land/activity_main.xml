<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/id_textLatitudeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:text="Latitude"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/id_textName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_textName"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/id_textLongitudeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:text="Longitude"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/id_textName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_textLatitude"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/id_textName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="32dp"
        android:text="Neel Shah"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/id_textLatitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="32dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/id_textLatitudeLabel"
        app:layout_constraintStart_toStartOf="@+id/id_textLatitudeLabel"
        app:layout_constraintTop_toBottomOf="@+id/id_textLatitudeLabel"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Latitude" />

    <TextView
        android:id="@+id/id_textLongitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="32dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/id_textLongitudeLabel"
        app:layout_constraintStart_toStartOf="@+id/id_textLongitudeLabel"
        app:layout_constraintTop_toBottomOf="@+id/id_textLongitudeLabel"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Longitude" />

    <TextView
        android:id="@+id/id_textAddressLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:text="Address"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/id_textName"
        app:layout_constraintTop_toBottomOf="@+id/id_textName"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/id_textAddress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="32dp"
        android:gravity="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/id_textName"
        app:layout_constraintTop_toBottomOf="@+id/id_textAddressLabel"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Address" />

    <TextView
        android:id="@+id/id_textDistanceLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:text="Distance"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/id_textAddressLabel"
        app:layout_constraintStart_toStartOf="@+id/id_textAddressLabel"
        app:layout_constraintTop_toBottomOf="@+id/id_textAddress"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/id_textDistance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="32dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/id_textDistanceLabel"
        app:layout_constraintStart_toStartOf="@+id/id_textDistanceLabel"
        app:layout_constraintTop_toBottomOf="@+id/id_textDistanceLabel"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Distance" />

    <Button
        android:id="@+id/id_buttonReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reset"
        app:layout_constraintBottom_toTopOf="@+id/id_textLongitudeLabel"
        app:layout_constraintEnd_toEndOf="@+id/id_textName"
        app:layout_constraintStart_toStartOf="@+id/id_textName"
        app:layout_constraintTop_toBottomOf="@+id/id_textLatitude" />

    <TextView
        android:id="@+id/id_textTimeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        android:textSize="18sp"
        android:textStyle="bold"
        android:visibility="gone"
        tools:layout_editor_absoluteX="366dp"
        tools:layout_editor_absoluteY="408dp" />

    <TextView
        android:id="@+id/id_textTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:visibility="gone"
        tools:layout_editor_absoluteX="366dp"
        tools:layout_editor_absoluteY="408dp"
        tools:text="Time" />

</androidx.constraintlayout.widget.ConstraintLayout>